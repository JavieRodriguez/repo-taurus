<p id="notice"><%= notice %></p>
<h1>Seleccione el proceso.</h1>
<%= link_to 'Registre un nuevo proceso', new_modalidad_proceso_path, class: 'btn btn-primary' %> 
<table class="table">
  <thead>
    <tr>
      <th>Numero</th>
      <th>Palabra Clave</th>
      <th>Objeto</th>
      <th>Plazo</th>
      <th>Tipo</th>
      <th>Acciones</th>
      <th colspan="6"></th>
    </tr>
  </thead>
  <tbody>
    <% @procesos.each do |proceso| %>
      <tr>
        <td><%= proceso.numero %></td>
        <td><%= proceso.palabraclave %></td>
        <td><%= proceso.objeto %></td>
        <td><%= proceso.plazo %></td>
        <td><%= proceso.tipocontrato.tipo %></td>
        <!-- /posts/:post_id/comments/:id -->
        <td><%= link_to 'Mostrar', [proceso.modalidad, proceso] %>
        <!-- /posts/:post_id/comments/:id/edit -->
        <%= link_to 'Editar', edit_modalidad_proceso_path(proceso.modalidad,proceso) %>
        <!-- /posts/:post_id/comments/:id -->
        <%= link_to 'Eliminar', [proceso.modalidad, proceso], method: :delete, data: { confirm: 'Esta seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br />
<%= link_to 'Volver al listado de modalidades.', modalidads_path %>